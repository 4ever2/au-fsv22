{
  "image": "coqorg/coq:8.15.2-ocaml-4.14.0-flambda",
  "customizations": {
    "vscode": {
      "extensions": ["maximedenes.vscoq"],
      "settings": {
        "files.exclude": {
          "**/*.aux": true,
          "**/*.glob": true,
          "**/*.vo": true,
          "**/*.vos": true,
          "**/*.vok": true,
          "**/.*.cache": true
        },
        "coq.loadCoqProject": true,
        "[coq]": {
          "editor.tabSize": 2,
          "editor.insertSpaces": true
        }
      }
    }
  }
}
