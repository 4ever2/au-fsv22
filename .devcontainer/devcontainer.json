{
  "image": "eskehoy/aufsv22:9.0.0",
  "customizations": {
    "vscode": {
      "extensions": ["maximedenes.vscoq"],
      "settings": {
        "files.exclude": {
          "**/*.aux": true,
          "**/*.glob": true,
          "**/*.vo": true,
          "**/*.vos": true,
          "**/*.vok": true,
          "**/.*.cache": true
        },
        "[coq]": {
          "editor.tabSize": 2,
          "editor.insertSpaces": true
        }
      }
    }
  }
}
